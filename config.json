{
  "name": "Zigate MQTT",
  "version": "0.0dev5",
  "slug": "zigate_mqtt",
  "description": "Zigate To MQTT Addon",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "boot": "auto",
  "options": {
    "mqtt_host": "hassio.local:1883",
    "port": "/dev/ttyUSB0"
